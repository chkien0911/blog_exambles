@model AsyncPartialView.Controllers.Dashboard

@{
    ViewBag.Title = "Index";
}

<h2>User</h2>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>


@foreach (var item in Model.Users) {
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
    </tr>
}

</table>

<h2>Course</h2>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Title</th>
    </tr>


    @foreach (var item in Model.Courses)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
        </tr>
    }

</table>
